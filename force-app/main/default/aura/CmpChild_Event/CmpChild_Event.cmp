<aura:component >
    
    <aura:registerEvent name="cmpEvent" type="c:ComponentEvent_1"/>
    
    <aura:attribute name="messageString" type="String"/>
    
    <h1>
    	Component Event Notifier
    </h1>
    
    <!--<ui:inputText label="Message" value="{!v.messageString}"/>
    
    <ui:button label="Fire Component Event" press ="{!c.fireComponentEvent}"/>-->
    <lightning:button variant="brand" label="Blue Button" title="Brand action" onclick="{! c.fireComponentEvent }" />
    <lightning:button variant="destructive" label="Red Button" title="Destructive action" onclick="{! c.fireComponentEvent }"/>
    <lightning:button variant="success" label="Green Button" title="Success" onclick="{! c.fireComponentEvent }"/>
</aura:component>